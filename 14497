#include <bits/stdc++.h>

using namespace std;

char arr[301][301];
int n,m;
int visited[301][301];
int dx[]={-1,0,1,0};
int dy[]={0,-1,0,1};
int sign;
int answer;

void dfs(int x,int y,int tmp)
{
	visited[x][y]=tmp;
	//cout<<x<<" "<<y<<"\n";
	
	if(arr[x][y]=='#')
	{
		cout<<tmp;
		sign=1;
		return;
	}
	if(arr[x][y]=='1')
	{
		arr[x][y]='0';
		return;
	}
	
	for(int i=0;i<4;i++)
	{
		int nx=x+dx[i];
		int ny=y+dy[i];
		
		if(nx>=0 && nx<n && ny>=0 &&ny<m)
		{
			if(visited[nx][ny]!=tmp)
			{
				dfs(nx,ny,tmp);
			}
		}
	}
}

int main()
{
	int a,b,c,d;
	cin>>n>>m;
	cin>>a>>b>>c>>d;
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<m;j++)
		{
			cin>>arr[i][j];
		}
	}
	
	arr[a-1][b-1]='0';
	
	for(int i=1;;i++)
	{
		dfs(a-1,b-1,i);
		
		if(sign==1)
		{
			break;
		}
	}
	
	
	
	return 0;
}
