#include <bits/stdc++.h>

using namespace std;

int main()
{
	int n;
	
	while(scanf("%d",&n)!=EOF)
	{
		int ret=1;
		int cnt=1;
		
		while(1)
		{
			if(cnt%n==0)
			{
				printf("%d\n",ret);
				break;
			}
			else
			{
				cnt=(cnt*10)+1;
				cnt=cnt%n;
				ret++;
			}
		}
		
		
	}
	
	
	
	
	return 0;
}
